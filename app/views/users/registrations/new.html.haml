.page-header
  %h1 Sign up
.row
  .span10
    = twitter_bootstrap_form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => {:class => "form-stacked"}) do |f|
      = f.inputs do
        = f.email_field :email
      = f.actions do
        = f.submit "Sign up"
  .span4
    = render :partial => "users/shared/links"