.page-header
  %h1 Sign in
.row
  .span10
    = twitter_bootstrap_form_for(resource, :as => resource_name, :url => session_path(resource_name), :html => {:class => "form-stacked"}) do |f|
      = f.inputs do
        = f.email_field :email

        = f.password_field :password

        = f.toggles "Rmember Me" do
          = f.check_box :remember_me

      = f.actions do
        = f.submit "Sign in"
  .span4
    = render :partial => "users/shared/links"